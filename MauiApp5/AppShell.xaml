<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp5.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp5.Pages"
    Shell.FlyoutBehavior="Flyout"
    Shell.TabBarIsVisible="False">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        
        <ShellContent
            Title="League of Legends"
            ContentTemplate="{DataTemplate local:LoLPage}"
            Icon="lol.jfif"/>
        
        <ShellContent
            Title="Valorant"
            ContentTemplate="{DataTemplate local:Valo}"
            Icon="valo.jfif"/>
        
        <ShellContent
            Title="Dota"
            ContentTemplate="{DataTemplate local:Dota}"
            Icon="dota.jfif"/>
        
    </FlyoutItem>

</Shell>
